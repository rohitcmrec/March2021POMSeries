pipeline{
    
    agent any
    
    stages{
        stage("Build"){
            steps{
                echo "Build the project"
            }
        }
        
        stage("Deploy on QA"){
            steps{
                echo "deploy on the QA env"
            }
        }
        
        stage("Unit Testing"){
            steps{
                echo "run unit test"
            }
        }
        
    }
    
}